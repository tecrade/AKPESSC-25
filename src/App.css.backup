/*
  AKPESSC 2025 - Advanced Interactive Design System V3
  Featuring dynamic particle backgrounds, game-like effects, and immersive interactions
*/

@import 'bootstrap/dist/css/bootstrap.min.css';
@import 'bootstrap-icons/font/bootstrap-icons.css';
@import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&family=Playfair+Display:wght@400;500;600;700&family=Orbitron:wght@400;500;600;700;800;900&display=swap');

:root {
  /* Advanced Hacker Color Palette with Game Effects */
  --hacker-green: #00ff41;
  --hacker-green-dark: #00b32e;
  --hacker-green-light: #66ff99;
  --hacker-green-glow: #00ff41;
  --matrix-black: #000000;
  --matrix-dark: #0a0a0a;
  --matrix-gray: #1a1a1a;
  --matrix-light-gray: #2a2a2a;
  --terminal-green: #00ff00;
  --terminal-bright: #00ff88;
  --accent-cyan: #00ffff;
  --accent-red: #ff0040;
  --accent-yellow: #ffff00;
  --accent-purple: #8000ff;
  
  /* Game Effect Colors */
  --lava-orange: #ff4500;
  --lava-red: #ff2200;
  --lava-yellow: #ffaa00;
  --pearl-white: #ffffff;
  --pearl-silver: #c0c0c0;
  --pearl-gold: #ffd700;
  --energy-blue: #0080ff;
  --energy-purple: #8000ff;
  
  /* Neutral Colors */
  --white: #ffffff;
  --black: #000000;
  --gray-light: #f8f9fa;
  --gray-medium: #6c757d;
  --gray-dark: #343a40;
  
  /* Spacing */
  --space-xs: 0.5rem;
  --space-sm: 1rem;
  --space-md: 1.5rem;
  --space-lg: 3rem;
  --space-xl: 4.5rem;
  --space-2xl: 6rem;
  --space-3xl: 9rem;
  
  /* Transitions */
  --transition-fast: 0.15s ease-in-out;
  --transition-normal: 0.3s ease-in-out;
  --transition-slow: 0.5s ease-in-out;
  
  /* Dynamic variables for interactive effects */
  --mouse-x: 50%;
  --mouse-y: 50%;
  --gradient-1-pos: 20% 80%;
  --gradient-2-pos: 80% 20%;
  --gradient-3-pos: 40% 40%;
  --gradient-4-pos: 60% 70%;
  --gradient-5-pos: 30% 30%;
  --mouse-intensity: 0;
  --scroll-y: 0px;
  --scroll-intensity: 0;
}

/* Global Styles */
* {
  box-sizing: border-box;
}

/* Force transparent background */
html, body {
  background: transparent !important;
  background-color: transparent !important;
}

/* Ensure main container is transparent */
.min-h-screen {
  background: transparent !important;
  background-color: transparent !important;
}

/* Override any potential solid backgrounds */
#root, .App, .container, .container-fluid {
  background: transparent !important;
  background-color: transparent !important;
}

/* Lightning Cursor System */
* {
  cursor: none !important;
}

body {
  cursor: none !important;
}

/* Ensure cursor is always visible */
html, body {
  cursor: none !important;
}

/* Fast cursor transitions */
* {
  transition: none !important;
}

/* Lightning Cursor Styles - Handled by LightningCursor component */

/* Dynamic Background Animations */

  25% {
    background-position: 100% 0%, 0% 100%, 25% 75%, 50% 50%, 25% 75%, 20% 20%, 0% 0%;
  }
  50% {
    background-position: 100% 100%, 0% 0%, 75% 25%, 75% 25%, 50% 50%, 40% 40%, 0% 0%;
  }
  75% {
    background-position: 0% 100%, 100% 0%, 75% 75%, 0% 100%, 75% 25%, 60% 60%, 0% 0%;
  }
}

@keyframes gradientWave {
  0%, 100% {
    background-size: 800% 800%, 600% 600%, 500% 500%, 900% 900%, 1000% 1000%, 300% 300%, 100% 100%;
  }
  50% {
    background-size: 1000% 1000%, 800% 800%, 700% 700%, 1100% 1100%, 1200% 1200%, 400% 400%, 100% 100%;
  }
}

@keyframes backgroundShift {
  0%, 100% {
    background-position: 0% 0%, 100% 100%, 50% 50%, 25% 75%, 75% 25%, 0% 0%, 0% 0%;
  }
  33% {
    background-position: 50% 25%, 25% 75%, 75% 50%, 50% 25%, 25% 75%, 15% 15%, 0% 0%;
  }
  66% {
    background-position: 75% 50%, 50% 25%, 25% 75%, 75% 50%, 50% 25%, 30% 30%, 0% 0%;
  }
}


  50% {
    background-size: 1000% 1000%, 800% 800%, 700% 700%, 1100% 1100%, 1200% 1200%, 400% 400%, 100% 100%;
  }
}

/* Selection Colors */
::selection {
  background: rgba(0, 255, 65, 0.3);
  color: #FFFFFF;
}

::-moz-selection {
  background: rgba(0, 255, 65, 0.3);
  color: #FFFFFF;
}

/* Mobile Responsiveness */
@media (max-width: 768px) {
  .custom-cursor,
  .cursor-trail,
  .cursor-trail-line {
    display: none !important;
  }
  
  * {
    cursor: auto !important;
  }
}

/* Ensure cursor visibility on all elements */
*:hover {
  cursor: none !important;
}

/* Fast cursor performance */
canvas {
  will-change: transform;
  transform: translateZ(0);
}

body {
  font-family: 'Inter', 'Courier New', 'Monaco', 'Menlo', monospace;
  line-height: 1.6;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  background: #000000;
  color: #ffffff;
  margin: 0;
  padding: 0;
  font-weight: 400;
  position: relative;
  overflow-x: hidden;
}


/* Removed unused interactive background effects for better performance */

/* Game-like Scrolling Effects */
.scroll-reveal {
  opacity: 0;
  transform: translateY(50px);
  transition: all 0.8s cubic-bezier(0.4, 0, 0.2, 1);
}

.scroll-reveal.revealed {
  opacity: 1;
  transform: translateY(0);
}

.scroll-reveal-left {
  opacity: 0;
  transform: translateX(-50px);
  transition: all 0.8s cubic-bezier(0.4, 0, 0.2, 1);
}

.scroll-reveal-left.revealed {
  opacity: 1;
  transform: translateX(0);
}

.scroll-reveal-right {
  opacity: 0;
  transform: translateX(50px);
  transition: all 0.8s cubic-bezier(0.4, 0, 0.2, 1);
}

.scroll-reveal-right.revealed {
  opacity: 1;
  transform: translateX(0);
}

.scroll-reveal-scale {
  opacity: 0;
  transform: scale(0.8);
  transition: all 0.8s cubic-bezier(0.4, 0, 0.2, 1);
}

.scroll-reveal-scale.revealed {
  opacity: 1;
  transform: scale(1);
}

/* Advanced Neon Effects */
.neon-glow {
  text-shadow: 
    0 0 5px currentColor,
    0 0 10px currentColor,
    0 0 15px currentColor,
    0 0 20px currentColor;
  animation: neonFlicker 2s ease-in-out infinite alternate;
}

/* Animated Title Effects */
.animated-title {
  animation: titleColorShift 4s ease-in-out infinite;
}

@keyframes titleColorShift {
  0% {
    color: #00FF41;
    text-shadow: 
      0 0 5px rgba(0, 255, 65, 0.8),
      0 0 10px rgba(0, 255, 65, 0.6),
      0 0 15px rgba(0, 255, 65, 0.4),
      0 0 20px rgba(0, 255, 65, 0.3);
  }
  25% {
    color: #00FF88;
    text-shadow: 
      0 0 5px rgba(0, 255, 136, 0.8),
      0 0 10px rgba(0, 255, 136, 0.6),
      0 0 15px rgba(0, 255, 136, 0.4),
      0 0 20px rgba(0, 255, 136, 0.3);
  }
  50% {
    color: #00FF00;
    text-shadow: 
      0 0 5px rgba(0, 255, 0, 0.8),
      0 0 10px rgba(0, 255, 0, 0.6),
      0 0 15px rgba(0, 255, 0, 0.4),
      0 0 20px rgba(0, 255, 0, 0.3);
  }
  75% {
    color: #00FF88;
    text-shadow: 
      0 0 5px rgba(0, 255, 136, 0.8),
      0 0 10px rgba(0, 255, 136, 0.6),
      0 0 15px rgba(0, 255, 136, 0.4),
      0 0 20px rgba(0, 255, 136, 0.3);
  }
  100% {
    color: #00FF41;
    text-shadow: 
      0 0 5px rgba(0, 255, 65, 0.8),
      0 0 10px rgba(0, 255, 65, 0.6),
      0 0 15px rgba(0, 255, 65, 0.4),
      0 0 20px rgba(0, 255, 65, 0.3);
  }
}

@keyframes neonFlicker {
  0%, 18%, 22%, 25%, 53%, 57%, 100% {
    text-shadow: 
      0 0 5px currentColor,
      0 0 10px currentColor,
      0 0 15px currentColor,
      0 0 20px currentColor;
  }
  20%, 24%, 55% {
    text-shadow: 
      0 0 2px currentColor,
      0 0 5px currentColor,
      0 0 8px currentColor,
      0 0 12px currentColor;
  }
}

.neon-border {
  border: 2px solid #00ff41;
  box-shadow: 
    0 0 10px #00ff41,
    inset 0 0 10px rgba(0, 255, 65, 0.1);
  animation: neonBorderPulse 2s ease-in-out infinite alternate;
}

@keyframes neonBorderPulse {
  0% {
    border-color: #00ff41;
    box-shadow: 
      0 0 10px #00ff41,
      inset 0 0 10px rgba(0, 255, 65, 0.1);
  }
  100% {
    border-color: #00ff88;
    box-shadow: 
      0 0 20px #00ff88,
      inset 0 0 20px rgba(0, 255, 136, 0.2);
  }
}

/* Enhanced Button Effects */
.shimmer-btn {
  position: relative;
  overflow: hidden;
  background: linear-gradient(45deg, #00b32e, #00ff41, #00ff88);
  background-size: 200% 200%;
  border: 2px solid #00ff41;
  color: #000000;
  font-weight: 600;
  font-size: clamp(1rem, 2vw, 1.2rem);
  padding: 1rem 2.5rem;
  border-radius: 10rem;
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  transition: all var(--transition-normal);
  box-shadow: 
    0 0 20px rgba(0, 255, 65, 0.4),
    0 0 40px rgba(0, 255, 65, 0.2);
  backdrop-filter: blur(5px);
  cursor: pointer;
  animation: shimmerMove 3s ease-in-out infinite;
}

@keyframes shimmerMove {
  0% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
  100% {
    background-position: 0% 50%;
  }
}

.shimmer-btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.4), transparent);
  transition: left 0.8s ease;
  z-index: 1;
}

.shimmer-btn:hover {
  background: linear-gradient(45deg, #00ff41, #00ff88, #00ff41);
  transform: translateY(-4px) scale(1.02);
  box-shadow: 
    0 0 30px #00ff41,
    0 0 60px rgba(0, 255, 65, 0.3),
    0 10px 20px rgba(0, 0, 0, 0.5);
}

.shimmer-btn:hover::before {
  left: 100%;
}

.shimmer-btn:active {
  transform: translateY(-1px) scale(0.98);
  box-shadow: 
    0 0 15px #00ff41,
    0 0 30px rgba(0, 255, 65, 0.2),
    0 5px 10px rgba(0, 0, 0, 0.3);
  transition: all 0.1s ease;
}

/* Enhanced Card Effects */
.card, .tm-card, .m-card, .event-card, .info-card, .contact-card, .venue-card, .faq-item {
  background: rgba(26, 26, 26, 0.8);
  border: 1px solid rgba(0, 255, 65, 0.3);
  border-radius: 1.5rem;
  padding: var(--space-lg);
  transition: all var(--transition-normal);
  position: relative;
  overflow: hidden;
  backdrop-filter: blur(10px);
}

.card::before, .tm-card::before, .m-card::before, .event-card::before, .info-card::before, .contact-card::before, .venue-card::before, .faq-item::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(0, 255, 65, 0.1), transparent);
  transition: left 0.6s ease;
  z-index: 1;
}

.card:hover, .tm-card:hover, .m-card:hover, .event-card:hover, .info-card:hover, .contact-card:hover, .venue-card:hover, .faq-item:hover {
  border-color: #00ff41;
  transform: translateY(-8px) scale(1.02);
  box-shadow: 
    0 15px 30px rgba(0, 0, 0, 0.4),
    0 0 20px rgba(0, 255, 65, 0.3),
    0 0 40px rgba(0, 255, 65, 0.1);
}

.card:hover::before, .tm-card:hover::before, .m-card:hover::before, .event-card:hover::before, .info-card:hover::before, .contact-card:hover::before, .venue-card:hover::before, .faq-item:hover::before {
  left: 100%;
}

/* Particle Canvas Styles */
.particle-canvas {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  pointer-events: none;
}

/* Spiral Green Elements */
.spiral-element {
  background: 
    radial-gradient(circle at 30% 30%, rgba(0, 255, 65, 0.9) 0%, rgba(0, 255, 65, 0.3) 30%, transparent 60%),
    radial-gradient(circle at 70% 70%, rgba(0, 255, 136, 0.7) 0%, rgba(0, 255, 136, 0.2) 30%, transparent 60%),
    radial-gradient(circle at 50% 50%, rgba(0, 255, 65, 0.5) 0%, rgba(0, 255, 65, 0.1) 40%, transparent 70%);
  border-radius: 50%;
  filter: blur(0.5px);
  animation: spiralFloat 8s ease-in-out infinite;
  box-shadow: 0 0 30px rgba(0, 255, 65, 0.3);
}

.spiral-element::before {
  content: '';
  position: absolute;
  top: 10%;
  left: 10%;
  right: 10%;
  bottom: 10%;
  background: 
    conic-gradient(from 0deg, 
      rgba(0, 255, 65, 0.8) 0deg,
      rgba(0, 255, 136, 0.6) 90deg,
      rgba(0, 255, 65, 0.8) 180deg,
      rgba(0, 255, 136, 0.6) 270deg,
      rgba(0, 255, 65, 0.8) 360deg
    );
  border-radius: 50%;
  animation: spiralRotate 6s linear infinite;
  box-shadow: inset 0 0 20px rgba(0, 255, 65, 0.4);
}

.spiral-element::after {
  content: '';
  position: absolute;
  top: 20%;
  left: 20%;
  right: 20%;
  bottom: 20%;
  background: 
    radial-gradient(circle, 
      rgba(0, 255, 65, 0.5) 0%,
      rgba(0, 255, 136, 0.3) 30%,
      rgba(0, 255, 65, 0.1) 60%,
      transparent 80%
    );
  border-radius: 50%;
  animation: spiralPulse 4s ease-in-out infinite;
  box-shadow: 0 0 15px rgba(0, 255, 65, 0.3);
}

@keyframes spiralFloat {
  0%, 100% {
    transform: translateY(0px) scale(1);
  }
  25% {
    transform: translateY(-20px) scale(1.1);
  }
  50% {
    transform: translateY(0px) scale(0.9);
  }
  75% {
    transform: translateY(20px) scale(1.05);
  }
}

@keyframes spiralRotate {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

@keyframes spiralPulse {
  0%, 100% {
    opacity: 0.3;
    transform: scale(1);
  }
  50% {
    opacity: 0.6;
    transform: scale(1.2);
  }
}

/* Material Design 3 Carousel Component */
.material-carousel {
  position: relative;
  width: 100%;
  max-width: 100%;
  margin: 0 auto;
  border-radius: 1rem;
  overflow: hidden;
  background: rgba(26, 26, 26, 0.8);
  border: 1px solid rgba(0, 255, 65, 0.3);
  backdrop-filter: blur(10px);
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
}

.carousel-container {
  position: relative;
  width: 100%;
  height: 100%;
}

.carousel-slides {
  position: relative;
  width: 100%;
  height: 400px;
  overflow: hidden;
  border-radius: 1rem;
}

.carousel-slide {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: transform 0.3s ease;
}

.carousel-slide:hover {
  transform: scale(1.02);
}

/* Navigation Arrows */
.carousel-arrow {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background: rgba(0, 255, 65, 0.9);
  border: none;
  border-radius: 50%;
  width: 50px;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  z-index: 10;
  transition: all 0.3s ease;
  color: #000000;
  font-size: 1.2rem;
  box-shadow: 0 4px 15px rgba(0, 255, 65, 0.3);
}

.carousel-arrow:hover {
  background: #00FF41;
  transform: translateY(-50%) scale(1.1);
  box-shadow: 0 6px 20px rgba(0, 255, 65, 0.5);
}

.carousel-arrow:active {
  transform: translateY(-50%) scale(0.95);
}

.carousel-arrow-left {
  left: 20px;
}

.carousel-arrow-right {
  right: 20px;
}

/* Indicators */
.carousel-indicators {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  gap: 12px;
  z-index: 10;
}

.carousel-indicator {
  width: 12px;
  height: 12px;
  border-radius: 50%;
  border: 2px solid rgba(255, 255, 255, 0.5);
  background: transparent;
  cursor: pointer;
  transition: all 0.3s ease;
  position: relative;
}

.carousel-indicator:hover {
  border-color: #00FF41;
  transform: scale(1.2);
}

.carousel-indicator.active {
  background: #00FF41;
  border-color: #00FF41;
  box-shadow: 0 0 10px rgba(0, 255, 65, 0.5);
}

.carousel-indicator.active::after {
  content: '';
  position: absolute;
  top: -4px;
  left: -4px;
  right: -4px;
  bottom: -4px;
  border: 2px solid rgba(0, 255, 65, 0.3);
  border-radius: 50%;
  animation: indicatorPulse 2s ease-in-out infinite;
}

@keyframes indicatorPulse {
  0%, 100% {
    transform: scale(1);
    opacity: 0.3;
  }
  50% {
    transform: scale(1.2);
    opacity: 0.6;
  }
}

/* Play/Pause Button */
.carousel-play-pause {
  position: absolute;
  top: 20px;
  right: 20px;
  background: rgba(0, 0, 0, 0.7);
  border: 1px solid rgba(0, 255, 65, 0.5);
  border-radius: 50%;
  width: 40px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  z-index: 10;
  transition: all 0.3s ease;
  color: #00FF41;
  font-size: 1rem;
}

.carousel-play-pause:hover {
  background: rgba(0, 255, 65, 0.2);
  border-color: #00FF41;
  transform: scale(1.1);
}

/* Responsive Design */
@media (max-width: 768px) {
  .carousel-slides {
    height: 300px;
  }
  
  .carousel-arrow {
    width: 40px;
    height: 40px;
    font-size: 1rem;
  }
  
  .carousel-arrow-left {
    left: 10px;
  }
  
  .carousel-arrow-right {
    right: 10px;
  }
  
  .carousel-indicators {
    bottom: 15px;
    gap: 8px;
  }
  
  .carousel-indicator {
    width: 10px;
    height: 10px;
  }
  
  .carousel-play-pause {
    top: 15px;
    right: 15px;
    width: 35px;
    height: 35px;
    font-size: 0.9rem;
  }
}

@media (max-width: 480px) {
  .carousel-slides {
    height: 250px;
  }
  
  .carousel-arrow {
    width: 35px;
    height: 35px;
    font-size: 0.9rem;
  }
  
  .carousel-indicators {
    bottom: 10px;
    gap: 6px;
  }
  
  .carousel-indicator {
    width: 8px;
    height: 8px;
  }
}

/* Accessibility */
.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  white-space: nowrap;
  border: 0;
}

/* Focus styles for keyboard navigation */
.carousel-arrow:focus,
.carousel-indicator:focus,
.carousel-play-pause:focus {
  outline: 2px solid #00FF41;
  outline-offset: 2px;
}

.material-carousel:focus {
  outline: 2px solid #00FF41;
  outline-offset: 2px;
}

/* Carousel Content Styles */
.carousel-content {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.carousel-image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.carousel-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  border-radius: 1rem;
}

.carousel-overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background: linear-gradient(transparent, rgba(0, 0, 0, 0.8));
  padding: 2rem;
  z-index: 2;
  border-radius: 0 0 1rem 1rem;
}

.carousel-title {
  color: #00FF41;
  font-size: 1.8rem;
  font-weight: 700;
  margin-bottom: 0.5rem;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.5);
}

.carousel-description {
  color: #FFFFFF;
  font-size: 1.1rem;
  margin: 0;
  text-shadow: 0 1px 2px rgba(0, 0, 0, 0.5);
}

/* Responsive carousel content */
@media (max-width: 768px) {
  .carousel-overlay {
    padding: 1.5rem;
  }
  
  .carousel-title {
    font-size: 1.5rem;
  }
  
  .carousel-description {
    font-size: 1rem;
  }
}

@media (max-width: 480px) {
  .carousel-overlay {
    padding: 1rem;
  }
  
  .carousel-title {
    font-size: 1.3rem;
  }
  
  .carousel-description {
    font-size: 0.9rem;
  }
}

/* Typography - Enhanced Visibility */
h1, h2, h3, h4, h5, h6 {
  font-family: 'Playfair Display', serif;
  font-weight: 600;
  line-height: 1.3;
  margin-bottom: var(--space-md);
  color: #07F946;
  letter-spacing: -0.02em;
  text-shadow: none;
  filter: none;
}

h1 {

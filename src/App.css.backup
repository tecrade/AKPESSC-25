  cursor: pointer;
}

.glimpse-card:hover {
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.15);
  transform: translateY(-5px);
}

.glimpse-image {
  position: relative;
  height: 220px;
  overflow: hidden;
}

.glimpse-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: all var(--transition-normal);
}

.glimpse-overlay {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 255, 65, 0.8);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: all var(--transition-normal);
}

.glimpse-card:hover .glimpse-overlay {
  opacity: 1;
}

.glimpse-card:hover .glimpse-image img {
  transform: scale(1.1);
}

.glimpse-category {
  color: var(--white);
  font-size: 0.9rem;
  font-weight: 600;
  margin-bottom: var(--space-sm);
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

.glimpse-overlay i {
  color: var(--white);
  font-size: 2rem;
}

.glimpse-content {
  padding: var(--space-lg);
}

.glimpse-title {
  font-size: 1.1rem;
  font-weight: 600;
  color: #00ff41;
  margin: 0;
}

/* Statistics */
.glimpse-stats {
  background: var(--accent-cream);
  border-radius: 1.5rem;
  padding: var(--space-xl);
}

.stat-card {
  text-align: center;
}

.stat-number {
  font-size: 3rem;
  font-weight: 700;
  color: #00ff41;
  margin-bottom: var(--space-sm);
  font-family: 'Playfair Display', serif;
}

.stat-label {
  color: var(--gray-medium);
  font-weight: 500;
  margin: 0;
}

/* Lightbox */
.lightbox-overlay {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.9);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9999;
  padding: var(--space-lg);
}

.lightbox-content {
  position: relative;
  max-width: 90vw;
  max-height: 90vh;
}

.lightbox-close {
  position: absolute;
  top: -50px;
  right: 0;
  background: none;
  border: none;
  color: var(--white);
  font-size: 2rem;
  cursor: pointer;
  z-index: 10000;
}

.lightbox-image {
  max-width: 100%;
  max-height: 80vh;
  object-fit: contain;
  border-radius: 12px;
}

.lightbox-caption {
  text-align: center;
  margin-top: var(--space-md);
}

.lightbox-caption h4 {
  color: var(--white);
  margin: 0;
}

/* Contact Section - Professional Style */
.contact-section {
  background: var(--accent-cream);
  padding: var(--space-lg) 0;
}

.contact-card {
  background: rgba(26, 26, 26, 0.8);
  border-radius: 1.5rem;
  padding: var(--space-lg);
  text-align: center;
  box-shadow: none;
  border: 1px solid rgba(0, 255, 65, 0.3);
  transition: all var(--transition-normal);
  height: 100%;
}

.contact-card:hover {
  transform: translateY(-5px);
  box-shadow: none;
  border-color: #00ff41;
}

.contact-avatar {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  background: linear-gradient(135deg, #00ff41, #00ff88);
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto var(--space-lg);
  color: #000000;
  font-size: 2rem;
}

.contact-name {
  font-size: 1.25rem;
  font-weight: 700;
  color: #00ff41;
  margin-bottom: var(--space-sm);
}

.contact-title {
  color: #ffffff;
  font-weight: 500;
  margin-bottom: var(--space-lg);
}

.contact-details p {
  margin-bottom: var(--space-sm);
  color: #ffffff;
}

.contact-details a {
  color: #00ff88;
  text-decoration: none;
  transition: all var(--transition-fast);
}

.contact-details a:hover {
  color: #00ff41;
  text-decoration: underline;
}

/* Venue Card */
.venue-card {
  background: rgba(26, 26, 26, 0.8);
  border-radius: 1.5rem;
  padding: var(--space-lg);
  box-shadow: none;
  border: 1px solid rgba(0, 255, 65, 0.3);
}

.venue-name {
  font-size: 1.5rem;
  font-weight: 700;
  color: #00ff41;
  margin-bottom: var(--space-md);
}

.venue-address {
  color: #ffffff;
  margin-bottom: var(--space-lg);
}

.venue-description {
  color: #ffffff;
  line-height: 1.6;
}

.google-map {
  border-radius: 1.5rem;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  border: 1px solid rgba(9, 84, 61, 0.1);
}

/* FAQ Section */
.faq-container {
  max-width: 1000px;
  margin: 0 auto;
  display: grid;
  grid-template-columns: 1fr;
  gap: var(--space-sm);
}

@media (min-width: 768px) {
  .faq-container {
    grid-template-columns: 1fr 1fr;
    gap: var(--space-sm);
  }
}

.faq-item {
  background: rgba(26, 26, 26, 0.8);
  border-radius: 6px;
  margin-bottom: 0;
  box-shadow: none;
  border: 1px solid rgba(0, 255, 65, 0.3);
  overflow: hidden;
  transition: all var(--transition-normal);
}

.faq-item:hover {
  border-color: #00ff41;
  transform: translateY(-2px);
}

.faq-question {
  padding: var(--space-xs) var(--space-sm);
  cursor: pointer;
  display: flex;
  justify-content: space-between;
  align-items: center;
  transition: all var(--transition-fast);
}

.faq-question:hover {
  background: rgba(0, 255, 65, 0.05);
  transform: translateX(5px);
}

.faq-question h5 {
  margin: 0;
  color: #00ff41;
  font-weight: 600;
  font-size: 0.85rem;
  transition: color var(--transition-normal);
}

.faq-item.expanded .faq-question h5 {
  color: #00ff88;
}

.faq-question i {
  color: #00ff88;
  transition: all var(--transition-fast);
  transform-origin: center;
  font-size: 0.75rem;
}

.faq-question:hover i {
  color: #00ff41;
  transform: scale(1.1);
}

.faq-item.expanded .faq-question i {
  color: #00ff41;
  transform: rotate(180deg) scale(1.1);
}

.faq-answer {
  padding: 0 var(--space-sm) var(--space-xs);
  color: #ffffff;
  line-height: 1.3;
  overflow: hidden;
  transition: all var(--transition-normal);
}

.faq-answer p {
  margin: 0;
  font-size: 0.75rem;
  line-height: 1.3;
}

.faq-item.expanded {
  border-color: #00ff41;
  box-shadow: 
    0 0 20px rgba(0, 255, 65, 0.3),
    0 0 40px rgba(0, 255, 65, 0.1);
}

.faq-item.expanded .faq-question {
  background: rgba(0, 255, 65, 0.05);
}

/* Registration Section */
.pricing-section {
  background: var(--accent-cream);
  padding: var(--space-3xl) 0;
}

.pricing-card {
  background: var(--white);
  border-radius: 1.5rem;
  padding: var(--space-xl);
  text-align: center;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  border: 1px solid rgba(9, 84, 61, 0.1);
  transition: all var(--transition-normal);
  height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  min-height: 500px;
}

.pricing-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.15);
}

.pricing-card.featured {
  border-color: #00ff41;
  border-width: 2px;
  box-shadow: 0 15px 35px rgba(0, 255, 65, 0.2);
}

.pricing-card.featured:hover {
  transform: translateY(-5px);
  box-shadow: 0 25px 45px rgba(0, 255, 65, 0.3);
}

.popular-badge {
  position: absolute;
  top: -10px;
  left: 50%;
  transform: translateX(-50%);
  background: #00ff41;
  color: #000000;
  padding: 0.5rem 1.5rem;
  border-radius: 1rem;
  font-size: 0.875rem;
  font-weight: 600;
}

.pricing-header {
  margin-bottom: var(--space-lg);
}

.pricing-name {
  font-size: 1.5rem;
  font-weight: 700;
  color: #e0e0e0;
  margin-bottom: var(--space-sm);
}

.pricing-price {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: var(--space-sm);
}

.price {
  font-size: 2.5rem;
  font-weight: 700;
  color: #00ff88;
  font-family: 'Playfair Display', serif;
  text-shadow: 0 0 10px rgba(127, 133, 255, 0.5);
}

.original-price {
  font-size: 1.25rem;
  color: #a0a0a0;
  text-decoration: line-through;
}

/* Light theme pricing */
[data-bs-theme="light"] .pricing-name {
  color: #461e10;
}

[data-bs-theme="light"] .price {
  color: #09543d;
  text-shadow: 0 0 10px rgba(9, 84, 61, 0.5);
}

[data-bs-theme="light"] .original-price {
  color: #64748b;
}

.pricing-features {
  margin-bottom: var(--space-lg);
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.feature-list {
  list-style: none;
  padding: 0;
  margin: 0;
}

.feature-item {
  display: flex;
  align-items: center;
  padding: var(--space-sm) 0;
  color: #ffffff;
  font-size: 1rem;
}

.feature-item i {
  color: #00ff41;
  font-size: 1.1rem;
}

.benefit-card {
  background: var(--white);
  border-radius: 1rem;
  padding: var(--space-lg);
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
  border: 1px solid rgba(9, 84, 61, 0.1);
  transition: all var(--transition-normal);
  height: 100%;
}

.benefit-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 15px 35px rgba(0, 0, 0, 0.15);
}

.benefit-card h5 {
  font-size: 1.25rem;
  font-weight: 600;
  margin-bottom: var(--space-sm);
  color: #00ff41;
}

.benefit-card p {
  margin: 0;
  color: var(--gray-medium);
  font-size: 1rem;
}

/* Footer */
.site-footer {
  background: #1C1C1D;
  color: #ffffff;
  padding: var(--space-3xl) 0 var(--space-lg);
}

.footer-content {
  text-align: center;
}

.footer-logo {
  max-width: 200px;
  height: auto;
  margin-bottom: var(--space-lg);
  filter: brightness(0) invert(1);
}

/* Sponsor Logos */
.sponsor-logos {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: var(--space-lg);
  margin-bottom: var(--space-lg);
  flex-wrap: wrap;
}

.sponsor-logo {
  max-width: 120px;
  height: auto;
  filter: brightness(0) invert(1);
  opacity: 0.8;
  transition: all var(--transition-normal);
}

.sponsor-logo:hover {
  opacity: 1;
  transform: scale(1.05);
}

.footer-text h4 {
  font-size: 1.5rem;
  font-weight: 600;
  margin-bottom: var(--space-md);
  color: #00FF88;
}

.footer-text p {
  color: #FFFFFF;
  margin-bottom: var(--space-lg);
  font-size: 1.1rem;
  text-align: center;
}

.footer-links {
  display: flex;
  justify-content: center;
  gap: var(--space-lg);
  margin-bottom: var(--space-lg);
  flex-wrap: wrap;
}

.footer-links a {
  color: #FFFFFF;
  text-decoration: none;
  transition: color var(--transition-fast);
  font-weight: 500;
}

.footer-links a:hover {
  color: #07F946;
}

.social-links {
  display: flex;
  justify-content: center;
  gap: var(--space-md);
  margin-bottom: var(--space-lg);
}

.social-links a {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50px;
  height: 50px;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 50%;
  color: #FFFFFF;
  text-decoration: none;
  transition: all var(--transition-normal);
  font-size: 1.25rem;
}

.social-links a:hover {
  background: #07F946;
  color: #1C1C1D;
  transform: translateY(-2px);
}

.contact-info {
  margin-bottom: var(--space-lg);
  text-align: center;
}

.contact-info h6 {
  font-size: 1.1rem;
  font-weight: 600;
  margin-bottom: var(--space-sm);
  color: #07F946;
  text-align: center;
}

.contact-info p {
  margin-bottom: var(--space-xs);
  color: #FFFFFF;
  text-align: center;
}

.contact-info a {
  color: #FFFFFF;
  text-decoration: none;
  transition: color var(--transition-fast);
  text-align: center;
  display: block;
}

.contact-info a:hover {
  color: #07F946;
}

.venue-info {
  margin-bottom: var(--space-lg);
  text-align: center;
}

.venue-info h6 {
  font-size: 1.1rem;
  font-weight: 600;
  margin-bottom: var(--space-sm);
  color: #07F946;
  text-align: center;
}

.venue-info p {
  color: #FFFFFF;
  margin: 0;
  text-align: center;
}

.copyright {
  border-top: 1px solid rgba(255, 255, 255, 0.1);
  padding-top: var(--space-lg);
  text-align: center;
}

.copyright p {
  margin: 0;
  color: rgba(255, 255, 255, 0.6);
  text-align: center;
}

/* Override Bootstrap classes for center alignment */
.venue-info .text-white-50,
.copyright .text-white-50,
.contact-info .text-white-50,
.footer-text .text-white-50 {
  text-align: center !important;
}

.venue-info .text-white,
.copyright .text-white,
.contact-info .text-white,
.footer-text .text-white {
  text-align: center !important;
}

/* Force footer title color override */
.footer-text h4,
.footer-text h4.text-white,
.footer-text h4.text-white-50,
.site-footer .footer-text h4,
.site-footer h4 {
  color: #00FF88 !important;
}

/* Center align individual contact cards */
.contact-info .col-md-4 {
  text-align: center;
}

.contact-info .col-md-4 h6,
.contact-info .col-md-4 p,
.contact-info .col-md-4 a {
  text-align: center !important;
}

/* Responsive Design */
@media (max-width: 768px) {
  .events-track, .sessions-track {
    gap: var(--space-md);
  }
  
  .event-card, .session-card {
    min-width: 250px;
  }
  
  .hero-section {
    padding: var(--space-xl) 0;
  }
  
  .akpessc-logo {
    max-width: 250px;
  }
  
  h1 {
    font-size: 2.5rem;
  }
  
  h2 {
    font-size: 2rem;
  }
  
  .btn, .tm-btn, .a-button {
    padding: 1.2rem 2rem;
    font-size: 1.2rem;
  }
  
  .sponsor-logos {
    gap: var(--space-md);
  }
  
  .sponsor-logo {
    max-width: 100px;
  }
}

@media (max-width: 576px) {
  .events-track, .sessions-track {
    gap: var(--space-sm);
  }
  
  .event-card, .session-card {
    min-width: 200px;
    padding: var(--space-md);
  }
  
  .hero-section {
    padding: var(--space-lg) 0;
  }
  
  .akpessc-logo {
    max-width: 200px;
  }
  
  h1 {
    font-size: 2rem;
  }
  
  h2 {
    font-size: 1.5rem;
  }
  
  .btn, .tm-btn, .a-button {
    padding: 1rem 1.5rem;
    font-size: 1rem;
  }
  
  .footer-links {
    flex-direction: column;
    gap: var(--space-sm);
  }
  
  .sponsor-logos {
    gap: var(--space-sm);
    flex-direction: column;
  }
  
  .sponsor-logo {
    max-width: 80px;
  }
}

/* Custom Cursor Styles */
.custom-cursor {
  position: fixed;
  width: 20px;
  height: 20px;
  background: var(--primary-green);
  border-radius: 50%;
  pointer-events: none;
  z-index: 9999;
  transition: transform 0.1s ease;
  mix-blend-mode: difference;
}

.custom-cursor.hover {
  transform: scale(2);
  background: var(--accent-pink);
}

/* Interactive Elements */
.interactive {
  cursor: pointer;
  transition: all var(--transition-normal);
  position: relative;
  overflow: hidden;
}

.interactive::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.1), transparent);
  transition: left var(--transition-slow);
}

.interactive:hover::before {
  left: 100%;
}

.interactive:hover {
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
}

/* Section Padding */
.section-padding {
  padding: var(--space-3xl) 0;
}

/* Container */
.container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 var(--space-md);
}

/* Hero Info Cards */
.info-card {
  background: rgba(26, 26, 26, 0.8);
  border-radius: 1rem;
  padding: var(--space-md);
  box-shadow: none;
  border: 1px solid rgba(0, 255, 65, 0.3);
  transition: all var(--transition-normal);
  height: 100%;
}

.info-card:hover {
  transform: translateY(-2px);
  box-shadow: none;
  border-color: #00ff41;
}

.info-card i {
  font-size: 2rem;
  color: #00ff41;
}

.info-card h6 {
  font-size: 1.1rem;
  font-weight: 600;
  margin-bottom: var(--space-xs);
  color: #00ff41;
}

.info-card p {
  margin: 0;
  font-size: 0.9rem;
  color: #ffffff;
}

/* Utility Classes */
.text-center {
  text-align: center;
}

.text-dark {
  color: #ffffff !important;
}

.text-muted {
  color: #cccccc !important;
}

.text-primary-green {
  color: #00ff41 !important;
}

.mb-4 {
  margin-bottom: var(--space-md) !important;
}

.mb-5 {
  margin-bottom: var(--space-lg) !important;
}

.mt-5 {
  margin-top: var(--space-lg) !important;
}

.me-3 {
  margin-right: var(--space-md) !important;
}

.lead {
  font-size: 1.25rem;
  font-weight: 300;
  line-height: 1.6;
}

/* Dark theme (Bootstrap-compatible) - Default */
:root {
  --bs-body-bg: #000000;
  --bs-body-color: #00ff41;
  --bs-primary: #00ff41;
  --bs-secondary: #00ff88;
  --bs-card-bg: #000000;
  --bs-link-color: #00ff41;
  --bs-link-hover-color: #00ff88;
  --bs-border-color: #1a1a1a;
}

/* Light theme override */
[data-bs-theme="light"] {
  --bs-body-bg: #ffffff;
  --bs-body-color: #1e293b;
  --bs-primary: #09543d;
  --bs-secondary: #64748b;
  --bs-card-bg: #ffffff;
  --bs-link-color: #09543d;
  --bs-link-hover-color: #1a7a5c;
  --bs-border-color: #e2e8f0;
}

/* Default dark theme styles */
body,
.hero-section,
.about-section2,
.events-section,
.glimpse-section,
.pricing-section,
.contact-section,
.section-padding {
  background: var(--bs-body-bg) !important;
  color: var(--bs-body-color) !important;
}

.card,
.pricing-card,
.benefit-card,
.info-card {
  background: var(--bs-card-bg);
  border-color: var(--bs-border-color);
}

.text-dark { color: var(--bs-body-color) !important; }
.text-muted { color: var(--bs-secondary) !important; }

/* Light theme overrides */
[data-bs-theme="light"] body,
[data-bs-theme="light"] .hero-section,
[data-bs-theme="light"] .about-section2,
[data-bs-theme="light"] .events-section,
[data-bs-theme="light"] .glimpse-section,
[data-bs-theme="light"] .pricing-section,
[data-bs-theme="light"] .contact-section,
[data-bs-theme="light"] .section-padding {
  background: var(--bs-body-bg) !important;
  color: var(--bs-body-color) !important;
}

[data-bs-theme="light"] .card,
[data-bs-theme="light"] .pricing-card,
[data-bs-theme="light"] .benefit-card,
[data-bs-theme="light"] .info-card {
  background: var(--bs-card-bg);
  border-color: var(--bs-border-color);
}

[data-bs-theme="light"] .text-dark { color: var(--bs-body-color) !important; }
[data-bs-theme="light"] .text-muted { color: var(--bs-secondary) !important; }

.small {
  font-size: 0.875rem;
}
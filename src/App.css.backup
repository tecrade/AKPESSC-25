  padding: 1.6rem 3rem;
  border-radius: 10rem;
  font-weight: 600;
  font-size: 1.4rem;
  text-decoration: none;
  transition: all var(--transition-normal);
  border: 2px solid transparent;
  position: relative;
  overflow: hidden;
  cursor: pointer;
}

/* Primary Button - Green */
.btn-primary, .tm-btn-primary, .a-button.-primary {
  background-color: #00ff41;
  color: #000000;
  border-color: #00ff41;
}

.btn-primary:hover, .tm-btn-primary:hover, .a-button.-primary:hover {
  background-color: #00b32e;
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(0, 255, 65, 0.3);
  color: #ffffff;
}

/* Secondary Button - Cyan */
.btn-secondary, .tm-btn-secondary, .a-button.-secondary {
  background-color: #00ffff;
  color: #000000;
  border-color: #00ffff;
}

.btn-secondary:hover, .tm-btn-secondary:hover, .a-button.-secondary:hover {
  background-color: #00cccc;
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(0, 255, 255, 0.3);
  color: #ffffff;
}

/* Tertiary Button - Purple */
.btn-tertiary, .tm-btn-tertiary, .a-button.-tertiary {
  background-color: #8000ff;
  color: #ffffff;
  border-color: #8000ff;
}

.btn-tertiary:hover, .tm-btn-tertiary:hover, .a-button.-tertiary:hover {
  background-color: #6600cc;
  color: #ffffff;
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(128, 0, 255, 0.3);
}

/* Enhanced Green Neon Shimmer Button Effect */
.shimmer-btn {
  position: relative;
  overflow: hidden;
  background: #00ff41;
  border: 2px solid #00ff41;
  color: #000000;
  font-weight: 600;
  font-size: 1rem;
  padding: 0.8rem 1.5rem;
  border-radius: 10rem;
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  box-shadow: 
    0 4px 14px 0 rgba(0, 255, 65, 0.3),
    0 0 20px rgba(0, 255, 65, 0.4),
    inset 0 1px 0 rgba(255, 255, 255, 0.2);
  letter-spacing: 0.025em;
  backdrop-filter: blur(10px);
}

/* Light theme shimmer button */
[data-bs-theme="light"] .shimmer-btn {
  background: linear-gradient(135deg, #00ff41 0%, #00b32e 100%);
  border: 2px solid #00ff41;
  box-shadow: 
    0 4px 14px 0 rgba(0, 255, 65, 0.3),
    0 0 20px rgba(0, 255, 65, 0.4),
    inset 0 1px 0 rgba(255, 255, 255, 0.2);
}

.shimmer-btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(
    90deg, 
    transparent, 
    rgba(255, 255, 255, 0.4), 
    rgba(255, 255, 255, 0.6),
    rgba(255, 255, 255, 0.4),
    transparent
  );
  transition: left 0.8s ease;
  z-index: 1;
}

.shimmer-btn::after {
  content: '';
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  background: linear-gradient(45deg, 
    #00ff41, 
    #00ff88, 
    #00ffff, 
    #00ff41
  );
  border-radius: 10rem;
  z-index: -1;
  opacity: 0;
  transition: opacity 0.3s ease;
  animation: borderGlow 2s ease-in-out infinite alternate;
}

.shimmer-btn:hover::before {
  left: 100%;
}

.shimmer-btn:hover::after {
  opacity: 1;
}

.shimmer-btn:hover {
  transform: translateY(-2px);
  box-shadow: 
    0 8px 25px rgba(0, 255, 65, 0.4),
    0 0 30px rgba(0, 255, 65, 0.5),
    inset 0 1px 0 rgba(255, 255, 255, 0.2);
  background: #00ff88;
  color: #000000;
}

.shimmer-btn:active {
  transform: translateY(0) scale(0.98);
  box-shadow: 
    0 4px 14px 0 rgba(0, 255, 65, 0.3),
    0 0 15px rgba(0, 255, 65, 0.4),
    inset 0 2px 4px rgba(0, 0, 0, 0.2);
  background: #00b32e;
  color: #ffffff;
  transition: all 0.1s ease;
}

.shimmer-btn:active::before {
  left: 100%;
  transition: left 0.1s ease;
}

.shimmer-btn:active::after {
  opacity: 0.8;
  animation: clickPulse 0.3s ease;
}

@keyframes borderGlow {
  0% {
    box-shadow: 0 0 5px #00ff41;
  }
  100% {
    box-shadow: 0 0 20px #00ff41, 0 0 30px #00ff88;
  }
}

@keyframes clickPulse {
  0% {
    transform: scale(1);
    box-shadow: 0 0 0 0 rgba(0, 255, 136, 0.7);
  }
  50% {
    transform: scale(1.05);
    box-shadow: 0 0 0 10px rgba(0, 255, 136, 0.3);
  }
  100% {
    transform: scale(1);
    box-shadow: 0 0 0 0 rgba(0, 255, 136, 0);
  }
}

/* Enhanced Card System with Neon Halo Effects */
.card, .tm-card, .m-card {
  background: rgba(26, 26, 26, 0.8);
  border: 1px solid rgba(0, 255, 65, 0.3);
  border-radius: 1.5rem;
  padding: var(--space-lg);
  transition: all var(--transition-normal);
  position: relative;
  overflow: hidden;
  box-shadow: none;
}

.card::before, .tm-card::before, .m-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 1px;
  background: linear-gradient(90deg, transparent, #00ff41, transparent);
  opacity: 0;
  transition: opacity var(--transition-normal);
}

.card::after, .tm-card::after, .m-card::after {
  content: '';
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  background: linear-gradient(45deg, 
    #00ff41, 
    #00ff88, 
    #00ffff, 
    #00ff41
  );
  border-radius: 1.5rem;
  z-index: -1;
  opacity: 0;
  transition: opacity var(--transition-normal);
  animation: cardGlow 3s ease-in-out infinite alternate;
}

.card:hover, .tm-card:hover, .m-card:hover {
  border-color: #00ff41;
  transform: translateY(-4px);
  box-shadow: none;
}

.card:hover::before, .tm-card:hover::before, .m-card:hover::before {
  opacity: 1;
}

.card:hover::after, .tm-card:hover::after, .m-card:hover::after {
  opacity: 0.6;
}

@keyframes cardGlow {
  0% {
    box-shadow: 0 0 5px #00ff41;
  }
  100% {
    box-shadow: 0 0 15px #00ff41, 0 0 25px #00ff88;
  }
}

